# Optional front-end token (sent as x-agent-office-token)
VITE_AGENT_OFFICE_TOKEN=

# Backend read token gate for UI/APIs
AGENT_OFFICE_READ_TOKEN=

# Backend ingest token gate for snapshot/event writes
AGENT_OFFICE_INGEST_TOKEN=

# Local OpenClaw file sources (used when cloud snapshot is not available)
AGENT_OFFICE_OPENCLAW_DIR=~/.openclaw
AGENT_OFFICE_WORKSPACE_DIR=~/clawd

# Push gateway config
AGENT_OFFICE_INGEST_URL=http://localhost:3000/api/ingest
AGENT_OFFICE_SOURCE_HOST=local-openclaw
AGENT_OFFICE_PUSH_INTERVAL_MS=15000

# Optional read/ingest rate limits
AGENT_OFFICE_READ_RATE_LIMIT=180
AGENT_OFFICE_READ_RATE_WINDOW_SEC=60
AGENT_OFFICE_INGEST_RATE_LIMIT=360
AGENT_OFFICE_INGEST_RATE_WINDOW_SEC=60

# Vercel KV (optional, required for persistent cloud live state)
KV_REST_API_URL=
KV_REST_API_TOKEN=

# Vercel Postgres (optional, required for timeline/snapshot history persistence)
POSTGRES_URL=
